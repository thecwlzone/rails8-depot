<div class="w-full">
  <%= render "notice" %>

  <h1 class="font-bold text-xl mb-6 pb-2 border-b-2">
    Your Pragmatic Catalog (Local
    <%= Rails.env.capitalize %>
    Environment)
  </h1>

  <%= turbo_stream_from "products" %>

  <ul>
    <% @products.each do |product| %>
      <%= render partial: "product", object: product %>
    <% end %>
  </ul>
</div>
